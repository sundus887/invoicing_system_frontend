{
  "buildCommand": "npm run build",
  "outputDirectory": "build",
  "framework": "create-react-app",
  "installCommand": "npm install",
  "devCommand": "npm start",
  "rewrites": [
    { "source": "/api/proxy/:path*", "destination": "https://hsoftworks.vercel.app/:path*" },
    { "source": "/api/:path*", "destination": "https://hsoftworks.vercel.app/api/:path*" }
  ]
}